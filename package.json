{
  "name": "particlify-image",
  "version": "1.0.0",
  "description": "A react component that transforms an image into a particle system with cool animations.",
  "repository": {
    "type": "git",
    "url": "https://github.com/mohamed7-dev/particlify-image.git"
  },
  "bugs": {
    "url": "https://github.com/mohamed7-dev/particlify-image/issues"
  },
  "homepage": "https://github.com/mohamed7-dev/particlify-image#readme",
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite example --open",
    "publish": "pnpm publish --access public --no-git-checks",
    "prepublishOnly": "pnpm build"
  },
  "keywords": [
    "react",
    "image",
    "typescript",
    "canvas",
    "particles"
  ],
  "author": "mohamed shaban",
  "license": "MIT",
  "packageManager": "pnpm@10.15.0",
  "peerDependencies": {
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.2",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "typescript": "^5.9.2",
    "vite": "^7.1.3",
    "vite-plugin-dts": "^4.5.4"
  }
}
